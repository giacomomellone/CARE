{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "AM - Build and Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/src/cpp/am/build/main",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/src/cpp/am",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build-AM",
        "miDebuggerPath": "/usr/bin/gdb",
        "miDebuggerArgs": "",
        "logging": { "trace": false, "traceResponse": false, "engineLogging": false },
        "linux": {
          "MIMode": "gdb"
        },
        "osx": {
          "MIMode": "lldb"
        },
        "windows": {
          "MIMode": "gdb"
        }
      }
    ]
  }
  